{
  "name": "school-ranking-app",
  "version": "1.0.0",
  "description": "A comprehensive school ranking web application for Mauritanian families to choose the best educational options",
  "main": "server.js",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "node --max-old-space-size=400 server.js",
    "start:production": "NODE_ENV=production node --max-old-space-size=400 server.js",
    "dev": "nodemon server.js",
    "build": "echo 'No build step required for Render Pro'",
    "build:netlify": "node build.js",
    "preview": "cd dist && python -m http.server 8000",
    "render-build": "echo 'No build step required for Render Pro'",
    "test": "node test-render.js"
  },
  "keywords": [
    "education",
    "ranking",
    "schools",
    "mauritania",
    "baccalaureate",
    "brevet",
    "cas"
  ],
  "author": "khalifa-IT09",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "multer": "^1.4.5-lts.1",
    "xlsx": "^0.18.5",
    "csv-parser": "^3.0.0",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "html-webpack-plugin": "^5.5.3",
    "css-loader": "^6.8.1",
    "style-loader": "^3.3.3",
    "babel-loader": "^9.1.3",
    "@babel/core": "^7.23.3",
    "@babel/preset-env": "^7.23.3"
  }
}



